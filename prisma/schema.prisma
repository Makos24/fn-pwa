// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Chip {
  id    Int     @id @default(autoincrement())
  email String  @unique
  phone String  @unique
  name  String
}

model Birth {
  id        Int     @id @default(autoincrement())
  name     String
  phone    String
  edd      String
}